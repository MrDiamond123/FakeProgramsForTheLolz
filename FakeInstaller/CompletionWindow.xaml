        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FakeInstaller"
        mc:Ignorable="d"
        Title="" Height="125" Width="400">
    <Grid>
        <Label x:Name="label" Content="Installation Complete" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" Height="100" Width="375" FontSize="39" TextOptions.TextFormattingMode="Display"/>

    </Grid>
</Window>
